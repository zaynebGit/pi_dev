{% extends 'index.html.twig' %}

{% block title %}Registration{% endblock %}

{% block body %}
    <h1>Registration</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ registration.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ registration.name }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ registration.email }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_registration_index') }}">back to list</a>

    <a href="{{ path('app_registration_edit', {'id': registration.id}) }}">edit</a>

    {{ include('registration/_delete_form.html.twig') }}
{% endblock %}
